{
  "schema_version": 1,
  "mixin": [
    "translucencyfix.mixins.json",
    "translucencyfix-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "translucencyfix",
    "version": "${version}",
    "name": "Translucency Fix",
    "description": "enables translucency for HUD and GUIs in Minecraft",
    "authors": ["ruvaldak"],
    "contact": {
      "sources": "https://github.com/ruvaldak/Semitranslucency"
    },
    "license": "MIT",
    "icon": "assets/translucencyfix/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "net.translucencyfix.quilt.translucencyfixQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.4"
      },
      {
        "id": "architectury",
        "version": ">=8.1.79"
      }
    ]
  }
}